# Add users in zzIT_P1_o365_LOAD

# Transcription -> LOG

$filename=get-date -Format 'yyyy-MM-dd-hh-mm'

Start-Transcript .\"$filename"_LOG.txt

$Count = 0

# Collect mailboxes from -> ExO P0 Script - Only Active Users

Write-Host "Importing CSV File"

$CW = Import-Csv D:\Raffa\Exchange_Online_Fix.csv

(Get-DistributionGroupMember -ResultSize Unlimited -Identity "zzIT_P1_o365_LOAD").count

Start-Sleep -s 5

ForEach ($CWF in $CW) {

    $Count++

    Write-Host "Adding user on zzIT_P1_o365_LOAD $($CWF.Name) number $Count/$($CW.Count)"

    $Alias = $CWF.SamAccountName

    # Adding user on zzIT_P1_o365_LOAD

	Add-DistributionGroupMember -Identity "zzIT_P1_o365_LOAD" -Member $Alias

}

Write-Host "Checking Count -> zzIT_P1_o365_LOAD"

Start-Sleep -s 15

Write-Host "zzIT_P1_o365_LOAD has..."

(Get-DistributionGroupMember -ResultSize Unlimited -Identity "zzIT_P1_o365_LOAD").count

Stop-Transcript